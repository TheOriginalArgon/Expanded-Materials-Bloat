<Defs>

  <ThingDef ParentName="RockBase">
    <defName>EM_MineableAlluminium</defName>
    <label>alluminium ore</label>
    <description>Rock containing bits of alluminium.</description>
    <graphicData>
      <texPath>Things/Building/Linked/RockFlecked_Atlas</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <colorTwo>(254,254,254)</colorTwo>
      <color>(128,128,128)</color>
    </graphicData>
    <statBases>
      <MaxHitPoints>900</MaxHitPoints>
    </statBases>
    <building>
      <isResourceRock>true</isResourceRock>
      <mineableThing>EM_Alluminium</mineableThing>
      <mineableYield>50</mineableYield>
      <mineableScatterCommonality>1.1</mineableScatterCommonality>
      <mineableScatterLumpSizeRange>12~25</mineableScatterLumpSizeRange>
    </building>
  </ThingDef>

